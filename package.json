{
  "name": "chaii",
  "packageManager": "bun@1.0.1",
  "private": true,
  "license": "GPL-3.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "ui:add": "bun --cwd=packages/ui run add"
  },
  "devDependencies": {
    "@chaii/eslint-config": "workspace:*",
    "@chaii/typescript-config": "workspace:*",
    "@types/lodash": "^4.17.1",
    "prettier": "^3.2.5",
    "turbo": "latest"
  },
  "dependencies": {
    "lodash": "^4.17.21"
  }
}
