{
  "name": "chaii",
  "packageManager": "bun@1.1.7",
  "private": true,
  "license": "GPL-3.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "typecheck": "turbo typecheck",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "clean": "rm -rf node_modules .turbo",
    "ui:add": "bun --filter=@chaii/ui run add"
  },
  "devDependencies": {
    "@chaii/eslint-config": "workspace:*",
    "@chaii/typescript-config": "workspace:*",
    "@types/bun": "^1.1.2",
    "@zemble/utils": "^0.7.34",
    "prettier": "^3.2.5",
    "turbo": "latest"
  }
}
